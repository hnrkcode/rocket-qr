{
  "name": "Rocket QR",
  "description": "Lightning-fast URL QR Code Generator",
  "version": "2.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "public/img/icon16.png",
    "32": "public/img/icon32.png",
    "48": "public/img/icon48.png",
    "128": "public/img/icon128.png"
  },
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "activeTab"
  ]
}

{
  "name": "Rocket QR",
  "description": "Lightning-fast URL QR Code Generator",
  "version": "2.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
